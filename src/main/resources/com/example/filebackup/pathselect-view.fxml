<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.filebackup.ui.PathSelectView"
            prefHeight="400.0" prefWidth="600.0">
    <VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:id="pathSelectLayout" >
        <Label fx:id="modeLabel" text="init label"/>
        <TextField fx:id="srcTextField"/>
        <Button  fx:id="srcPathSelect" onAction="#onSrcPathSelect"/>
        <TextField fx:id="dstTextField"/>
        <Button fx:id="dstPathSelect" onAction="#onDstPathSelect"/>
        <Button fx:id="action" onAction="#onAction" text="action"/>
    </VBox>

</AnchorPane>
