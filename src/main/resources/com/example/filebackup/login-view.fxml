<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.filebackup.ui.LoginView"
            prefHeight="400.0" prefWidth="600.0">
    <VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:id="loginLayout" >
        <Label fx:id="loginLabel" text="Log in"/>
        <TextField fx:id="usernameInput"/>
        <TextField fx:id="passwordInput"/>
        <Button fx:id="login" onAction="#loginAction" text="action"/>
    </VBox>

</AnchorPane>
